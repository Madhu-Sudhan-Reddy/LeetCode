class Solution {
    public int arrayPairSum(int[] nums) {
        //cherrypicktrial
        Arrays.sort(nums);
        int count=0;
        int i=nums.length-2;
        for(;i>=0;i=i-2){
            count=count+nums[i];
            System.out.println(nums[i]+" "+count);
        }
        return count;
    }
}